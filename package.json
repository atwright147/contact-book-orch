{
  "name": "docker",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "start": "./node_modules/babel-cli/bin/babel-node.js ./server.js",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register --watch",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "docker:npm:install": "docker exec -i -t docker_node_1 /bin/bash -c 'npm i'",
    "docker:rm:volumes": "docker volume rm $(docker volume ls -q) -f",
    "docker:ls:volumes": "docker volume ls -q",
    "docker:rm:images": "docker rmi $(docker images -a -q) -f",
    "docker:ls:images": "docker images -a -q"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "express": "^4.14.1",
    "mysql": "^2.13.0",
    "sequelize": "^3.30.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.17.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "sequelize-cli": "^2.5.1",
    "sinon": "^1.17.7"
  },
  "babel": {
    "retainLines": true,
    "presets": [
      "es2015",
      "es2016"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
