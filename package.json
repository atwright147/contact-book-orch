{
  "name": "contact-book-orch",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "start": "./node_modules/babel-cli/bin/babel-node.js ./server.js",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register --watch",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "build": "rimraf dist/ && babel src -d dist && npm run copy",
    "copy": "cp contact-book.service bookshelf.js knexfile.js common.env dist/",
    "docker:npm:install": "docker exec -i -t docker_node_1 /bin/bash -c 'npm i'",
    "docker:rm:volumes": "docker volume rm $(docker volume ls -q) -f",
    "docker:ls:volumes": "docker volume ls -q",
    "docker:rm:images": "docker rmi $(docker images -a -q) -f",
    "docker:ls:images": "docker images -a -q"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "body-parser": "^1.17.1",
    "bookshelf": "^0.12.0",
    "cors": "^2.8.3",
    "express": "^4.14.1",
    "knex": "^0.14.2",
    "mysql": "^2.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.14.0",
    "faker": "^4.1.0",
    "mocha": "^4.1.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.3"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-var": "warn"
    }
  }
}
